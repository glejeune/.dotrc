zshrc_load_status () {
  echo "\r.zshrc load: $* ... \e[0K"
}

fpath=(~/.zsh/functs $fpath)

for file in ~/.zsh/rc/*; 
do 
  source $file 
done

[ -f /usr/bin/dircolors ] && eval `dircolors ~/.zsh/dircolors.gen`

setopt autocd

PATH=$PATH:$HOME/.rvm/bin # Add RVM to PATH for scripting

[ -f ~/.zshrc.local ] && source ~/.zshrc.local
